{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    // "lib": [],
    "moduleResolution": "Bundler",
    "rootDir": "./src",
    "outDir": "./dist",

    "strict": true,
    "isolatedModules": true,
    "verbatimModuleSyntax": true,

    "sourceMap": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
